%h2 Sign in
= form_for resource, as: resource_name, url: session_path(resource_name) do |f|
  %label
    %span Username
    = f.text_field :username, autofocus: true

  %label
    %span Password
    = f.password_field :password

  %label
    %span Remember Me
    = f.check_box :remember_me

  = f.submit "Sign in"
